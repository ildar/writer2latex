<helpdocument version="1.0">
	<meta>
        <topic id="writer2latex-templatex" indexer="include">
            <title xml-lang="en-US">Using the LaTeX templates</title>
            <filename>org.openoffice.da.writer2latex.oxt/templates.xhp</filename>
